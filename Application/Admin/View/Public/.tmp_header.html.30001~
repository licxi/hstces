	<script type="text/javascript">
      $(function(){
        var clickMenu = $.cookie('sMenu');
		//默认第一个菜单展开
        if (clickMenu == null) {
            clickMenu = 0;
        }
        $('.has_sub a').each(function(i) {
			$(this).click(function() {
			if ($(this).next().css('display') == 'none') {
			  $.cookie('sMenu', i, { expires: 3600 * 24 * 30, path: '/' });
			} 
			});
			if (i == clickMenu) {
			$(this).addClass("subdrop");
			$(this).next().css('display', 'block');
			}		  
        });
      });
    </script>
